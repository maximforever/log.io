<h4>

<% @user.each do |u| %>


<% if logged_in? %>
	<% if u == current_user %>
		<div class = "col-md-3" id = "current"><%= u.id %>. <%= u.name %></div>
		<div class = "col-md-2" id = "current"><%= u.email %></div>
	<% else %>
		<div class = "col-md-3"><%= u.id %>. <%= u.name %></div>
		<div class = "col-md-2"><%= u.email %></div>
	<% end %>
<% else %>
		<div class = "col-md-3"><%= u.id %>. <%= u.name %></div>
		<div class = "col-md-2"><%= u.email %></div>


<% end %>




<br>

<% end %>
</h4>